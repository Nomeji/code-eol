{
	"name": "code-eol",
	"displayName": "code-eol",
	"description": "Display newline characters in your code",
	"version": "0.3.0",
	"publisher": "jeff-hykin",
	"repository": {
		"url": "https://github.com/jeff-hykin/code-eol"
	},
	"license": "MIT",
	"engines": {
		"vscode": "^1.15.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension",
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"devDependencies": {
		"vscode": "^1.1.5",
		"@types/node": "7.0.4"
	},
	"dependencies": {
		"vsce": "^1.40.0"
	},
	"contributes": {
		"configuration": [
			{
				"type": "object",
				"title": "code-eol Configuration",
				"properties": {
					"code-eol.color": {
						"type": [
							"string",
							"null"
						],
						"default": null
					},
					"code-eol.newlineCharacter": {
						"type": [
							"string",
							"null"
						],
						"default": null
					},
					"code-eol.returnCharacter": {
						"type": [
							"string",
							"null"
						],
						"default": null
					},
					"code-eol.crlfCharacter": {
						"type": [
							"string",
							"null"
						],
						"default": null
					}
				}
			}
		]
	}
}
